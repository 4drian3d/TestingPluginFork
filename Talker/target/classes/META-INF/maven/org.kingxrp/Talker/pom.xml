<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.kingxrp</groupId>
  <artifactId>Talker</artifactId>
  <version>0.1</version>
  <name>Talker</name>

  <repositories>

    <!--Repositorio de paper mc-->
    <repository>
      <id>paper-repository</id>
      <url>https://repo.papermc.io/repository/maven-public/</url>
    </repository>

  </repositories>



  <dependencies>

    <!--Dependencia de papermc (El software para servidores)-->
    <dependency>
      <groupId>io.papermc.paper</groupId>
      <artifactId>paper-api</artifactId>
      <version>1.19.4-R0.1-SNAPSHOT</version>
      <scope>paper-repository</scope>
    </dependency>

    <!--Dependencia de MySQL Connector para bases de datos-->
    <dependency>
      <groupId>mysql</groupId>
      <artifactId>mysql-connector-java</artifactId>
      <version>8.0.33</version> 
    </dependency>


    <!--Dependencia para colores en java-->
    <dependency>
      <groupId>org.fusesource.jansi</groupId>
      <artifactId>jansi</artifactId>
      <version>2.3.2</version>
    </dependency>


    <!--Dependencias de BungeeCord -->
    <dependency>
        <groupId>net.md-5</groupId>
        <artifactId>bungeecord-api</artifactId>
        <version>1.17-R0.1-SNAPSHOT</version>
        <scope>provided</scope>
    </dependency>


    
  </dependencies>



</project>